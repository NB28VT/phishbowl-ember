<h3>Manage Shows</h3>
<table class="table">
  <thead>
    <tr>
      <th>Date</th>
      <th>Venue</th>
    </tr>
  </thead>
  <tbody>
    {{#each model as |show|}}
      <tr>
        <td>{{show.date}}</td>
        <td>{{show.venue}}</td>
        <td><button class="btn btn-danger" {{action 'deleteShow' show}}>Remove</button></td>
      </tr>
    {{/each}}
  </tbody>
</table>


{{outlet}}
